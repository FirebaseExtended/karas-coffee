{
  "hosting": {
    "public": "dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/bundle/**",
        "function": "ext-firestore-bundle-server-serve"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
