{
  "hosting": {
    "public": "dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/bundles/*",
        "function": "ext-firestore-bundle-server-9rb7-serve"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
